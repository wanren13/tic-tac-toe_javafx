<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
          minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
          xmlns="http://javafx.com/javafx/11.0.1"
          xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="tictactoe.WelcomeController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <Button id="human" onMouseClicked="#onClicked" alignment="CENTER"
                mnemonicParsing="false" text="VS&#10;Human" textAlignment="CENTER"
                GridPane.halignment="CENTER" GridPane.valignment="CENTER">
            <font>
                <Font name="AR DESTINE" size="32.0" />
            </font>
        </Button>
        <Button id="computer" onMouseClicked="#onClicked" alignment="CENTER"
                mnemonicParsing="false" text="VS&#10;Computer" textAlignment="CENTER"
                GridPane.columnIndex="1" GridPane.halignment="CENTER"
                GridPane.valignment="CENTER">
            <font>
                <Font name="AR DESTINE" size="32.0" />
            </font>
        </Button>
    </children>
</GridPane>
