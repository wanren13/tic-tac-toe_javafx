<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
          minHeight="-Infinity" minWidth="-Infinity" prefHeight="250.0" prefWidth="300.0"
          xmlns="http://javafx.com/javafx/11.0.1"
          xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="tictactoe.ResultController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" percentHeight="60.0" vgrow="SOMETIMES"
        />
        <RowConstraints minHeight="10.0" percentHeight="15.0" vgrow="SOMETIMES"
        />
        <RowConstraints minHeight="10.0" percentHeight="15.0" vgrow="SOMETIMES"
        />
        <RowConstraints minHeight="10.0" percentHeight="10.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <Label fx:id="label" alignment="CENTER" contentDisplay="CENTER" text="X Wins"
               textFill="RED" GridPane.halignment="CENTER"
               GridPane.valignment="CENTER">
            <font>
                <Font name="AR DESTINE" size="72.0" />
            </font>
        </Label>
        <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                onMouseClicked="#onClicked" prefWidth="100.0" text="Play again"
                textAlignment="CENTER" GridPane.halignment="CENTER" GridPane.rowIndex="1"
                GridPane.valignment="CENTER">
            <font>
                <Font name="AR DESTINE" size="12.0" />
            </font>
        </Button>
        <Button mnemonicParsing="false" onMouseClicked="#onClicked" prefWidth="100.0"
                text="QUIT" GridPane.halignment="CENTER" GridPane.rowIndex="2"
                GridPane.valignment="CENTER">
            <font>
                <Font name="AR DESTINE" size="12.0" />
            </font>
        </Button>
    </children>
</GridPane>
